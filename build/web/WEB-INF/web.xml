<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>User.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>User.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_login</servlet-name>
        <servlet-class>Admin.Admin_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_venue</servlet-name>
        <servlet-class>Admin.Add_venue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_venue</servlet-name>
        <servlet-class>Admin.Delete_venue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_booking_queue</servlet-name>
        <servlet-class>User.add_booking_queue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>get_orders</servlet-name>
        <servlet-class>User.get_orders</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Remove_order</servlet-name>
        <servlet-class>User.Remove_order</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_services</servlet-name>
        <servlet-class>User.add_services</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>remove_added_service</servlet-name>
        <servlet-class>User.remove_added_service</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>booking</servlet-name>
        <servlet-class>User.booking</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_login</servlet-name>
        <url-pattern>/Admin_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_venue</servlet-name>
        <url-pattern>/Add_venue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_venue</servlet-name>
        <url-pattern>/Delete_venue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_booking_queue</servlet-name>
        <url-pattern>/add_booking_queue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>get_orders</servlet-name>
        <url-pattern>/get_orders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Remove_order</servlet-name>
        <url-pattern>/Remove_order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_services</servlet-name>
        <url-pattern>/add_services</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>remove_added_service</servlet-name>
        <url-pattern>/remove_added_service</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>booking</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
